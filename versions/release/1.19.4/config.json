{
    "official": true,
    "merge_patches": true,
    "java_target": 17,
    "encoding": "UTF-8",
    "fernflower": {
        "version": "net.minecraftforge:forgeflower:2.0.627.2",
        "java_version": 17,
        "args": ["-din=1", "-rbr=1", "-dgs=1", "-asc=1", "-rsy=1", "-iec=1", "-jvn=1", "-isl=0", "-iib=1", "-bsm=1", "-dcl=1", "-log=TRACE", "-cfg", "{libraries}", "{input}", "{output}"],
        "jvmargs": ["-Xmx4G"]
    },
    "merge": {
        "version": "net.minecraftforge:mergetool:1.1.5:fatjar",
        "args": ["--client", "{client}", "--server", "{server}", "--ann", "{version}", "--output", "{output}", "--inject", "false"],
        "jvmargs": []
    },
    "rename": {
        "version": "net.minecraftforge:ForgeAutoRenamingTool:0.1.22:all",
        "args": ["--input", "{input}", "--output", "{output}", "--map", "{mappings}", "--cfg", "{libraries}", "--ann-fix", "--ids-fix", "--src-fix", "--record-fix"]
    },
    "bundler_extract_jar": {
        "version": "net.minecraftforge:installertools:1.3.0:fatjar",
        "args": ["--task", "bundler_extract", "--input", "{input}", "--output", "{output}", "--jar-only"]
    },
    "bundler_extract_libs": {
        "version": "net.minecraftforge:installertools:1.3.0:fatjar",
        "args": ["--task", "bundler_extract", "--input", "{input}", "--output", "{output}", "--libraries"]
    },
    "libraries": {
        "client": ["com.google.code.findbugs:jsr305:3.0.2", "ca.weblite:java-objc-bridge:1.1", "org.jetbrains:annotations:23.0.0"],
        "server": ["com.google.code.findbugs:jsr305:3.0.2", "org.jetbrains:annotations:23.0.0"],
        "joined": ["com.google.code.findbugs:jsr305:3.0.2", "ca.weblite:java-objc-bridge:1.1", "net.minecraftforge:mergetool:1.1.5:api", "org.jetbrains:annotations:23.0.0"]
    }
}
